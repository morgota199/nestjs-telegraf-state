{
  "name": "nestjs-telegraf",
  "version": "2.6.3",
  "description": "Telegraf module for NestJS",
  "keywords": [
    "nest",
    "nestjs",
    "nestjs-module",
    "telegraf",
    "telegram",
    "telegram bot",
    "telegram bot api",
    "bot",
    "bot api",
    "bot framework"
  ],
  "homepage": "https://nestjs-telegraf.hypeer.company",
  "bugs": {
    "url": "https://github.com/hypeertech/nestjs-telegraf/issues"
